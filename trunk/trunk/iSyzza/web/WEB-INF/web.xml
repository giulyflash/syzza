<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>LoginClienteProcessor</servlet-name>
        <servlet-class>control.LoginClienteProcessor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastroProcessor</servlet-name>
        <servlet-class>control.CadastroProcessor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MainControl</servlet-name>
        <servlet-class>control.MainControl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginClienteProcessor</servlet-name>
        <url-pattern>/logincli.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastroProcessor</servlet-name>
        <url-pattern>/cadcli.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MainControl</servlet-name>
        <url-pattern>/main.dol</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
